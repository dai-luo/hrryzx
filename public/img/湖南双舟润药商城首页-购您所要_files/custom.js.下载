//处方药
function yinCang(){
    var yin = $("#yincang1").val();
    if("Y"== yin){
//			document.getElementById("yin1").style.display="none";
        document.getElementById("yin1").setAttribute("class","");
//			document.getElementById("yin1").style.width="560px";
        document.getElementById("yin1").style.height="1px";
        document.getElementById("yin1").style.margin="auto";
        document.getElementById("prd1").style.display="none";
        document.getElementById("tab1").style.display="none";
    }
}

//进口药
function yinCang2(){
    var yin = $("#yincang2").val();
    if("Y"== yin){
        document.getElementById("yin2").setAttribute("class","");
        document.getElementById("yin2").style.height="1px";
        document.getElementById("yin2").style.margin="auto";
        document.getElementById("prd2").style.display="none";
        document.getElementById("tab2").style.display="none";
    }
}

